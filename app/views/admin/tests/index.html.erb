<div class="container">
  <h2 class="mt-4"><%= t('.header') %></h2>

  <%= link_to t('.new'), new_admin_test_path, class: "btn btn-primary mb-5" %>

  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col"><%= t('.title') %></th>
        <th scope="col"><%= t('.level') %></th>
        <th scope="col"><%= t('.questions') %></th>
        <th scope="col" colspan="3" class="text-center"><%= t('.action') %></th>
      </tr>
    </thead>
    <tbody>
      <%= render @tests %>
    </tbody>
  </table>
</div>